//Rewrite to www
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^allevamentocapresaanen.it[nc]
RewriteRule ^(.*)$ http://www.allevamentocapresaanen.it/$1 [r=301,nc]

//Custom 400 errors
ErrorDocument 400 /404.html

//Custom 401 errors
ErrorDocument 401 /404.html

//Custom 403 errors
ErrorDocument 403 /404.html

//Custom 404 errors
ErrorDocument 404 /404.html

//Custom 500 errors
ErrorDocument 500 /404.html
